ALTER FUNCTION [dbo].[REMOVERCARACTERESRAROS](@TEMP VARCHAR(1000))
RETURNS VARCHAR(1000)
AS
BEGIN
    
DECLARE @ACCEPTABLECHARACTERS VARCHAR(100)

    
SET @ACCEPTABLECHARACTERS = 'Z0-9'
    
WHILE PATINDEX('%[^' + @ACCEPTABLECHARACTERS + ']%', 
@TEMP COLLATE LATIN1_GENERAL_BIN) > 0
        SET @TEMP = STUFF(@TEMP, PATINDEX('%[^' + @ACCEPTABLECHARACTERS + ']%', @TEMP COLLATE LATIN1_GENERAL_BIN), 1, '')

    RETURN @TEMP
END
